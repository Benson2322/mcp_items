{
    "mcpServers": {
        "mcp-server-fetch": {
            "command": "npx",
            "args": [
                "-y",
                "@kazuph/mcp-fetch"
            ]
        },
        "figma-developer-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "figma-developer-mcp",
                "--stdio"
            ],
            "env": {
                "FIGMA_API_KEY": "<your-figma-api-key>"
            }
        }
    }
}